    <td><%= cell_blood_count_test.monocyte %></td>
    <td><%= cell_blood_count_test.eosinophil %></td>
    <td><%= cell_blood_count_test.basophil %></td>
    <td><%= link_to 'Show', cell_blood_count_test %></td>
    <td><%= link_to 'Edit', edit_cell_blood_count_test_path(cell_blood_count_test) %></td>
    <td><%= link_to 'Destroy', cell_blood_count_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

