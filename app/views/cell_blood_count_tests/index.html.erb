<% @cell_blood_count_tests.each do |cell_blood_count_test| %>
  <tr>
    <td><%= cell_blood_count_test.extraction_date %></td>
    <td><%= cell_blood_count_test.red_blood_cells %></td>
    <td><%= cell_blood_count_test.hemoglobin %></td>
    <td><%= cell_blood_count_test.hematocrit %></td>
    <td><%= cell_blood_count_test.vcm %></td>
    <td><%= cell_blood_count_test.hcm %></td>
